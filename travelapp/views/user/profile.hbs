<h1>User Profile</h1>
<p>{{ user.name }}</p>

<p>{{ user.description }}</p>
<img src={{ user.profilePic }}>

<a href="/logged/social/update">Settings</a>
<a href="/logged/social/map">Map</a>
<a href="user/friends">Friends</a>

{{#each posts}}
    <a href="/logged/post/{{ _id }}">{{ title }}</a>
    <img src={{ image }}>
{{/each}}

